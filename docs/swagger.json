{
  "openapi": "3.0.2",
  "info": {
    "title": "Games-Api",
    "version": "1.0.0",
    "description": "### This is a Swagger UI for our Games API"
  },
  "paths": {
    "/games": {
      "get": {
        "tags": [
          "games"
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                },
                "examples": {
                  "list of games": {
                    "value": [
                      "nhl22",
                      "minecraft"
                    ]
                  }
                }
              }
            },
            "description": ""
          }
        },
        "description": "Return a list of all games in API"
      }
    }
  }
}